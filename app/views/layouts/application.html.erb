<!DOCTYPE html>
<html>
  <head>
    <title>HomeworkAppSunday</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" <%= link_to "Want Blogs?", blogs_path %></a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <div class="nav-link"><%= link_to "Home", root_path %></div>
          <div class="nav-link"><%= link_to "Contact", pages_contact_path %></div>
          <div class="nav-link"><%= link_to "About", pages_about_path %></div>
          <div class="nav-link"><%= link_to "FAQ", pages_faq_path %></div>
        </div>
      </div>
    </nav>
    
    <%= yield %>
    
    
  </body>
</html>
